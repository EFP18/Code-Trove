{{!-- using materialize --}}
{{!-- rendering all posts for the home page --}}
{{#each posts}}
<div class="row">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">{{this.title}}</span>
        <p>{{this.body}}</p>
      </div>
      <div class="card-action">
        <a href="#">Like</a>
        <a href="#">Save</a>
      </div>
    </div>
  </div>
</div>
{{/each}}




