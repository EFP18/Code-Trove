{{!-- using materialize --}}

{{#each post}}
<div class="row">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Snippet Title</span>
        <p>{{this}}</p>
      </div>
      <div class="card-action">
        <a href="#">Like</a>
        <a href="#">Save</a>
      </div>
    </div>
  </div>
</div>
{{/each}}




